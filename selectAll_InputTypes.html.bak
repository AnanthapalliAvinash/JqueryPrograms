<!DOCTYPE html>
<html>

<head>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <script>
        $(document).ready(function () {
            $(":input");
            $(":button").css('background-color',"yellow");
			$(":radio");
			$(":text").val("xyz");
			$("div :checkbox").prop("checked","true");
			$(":image").css("border", "solid 10px red");
			$(":file").css("border", "solid 1px red");
			$(":enabled").attr("placeholder", "Enter Value");
			$(":disabled").attr("placeholder", "disabled");
			$(":selected").css("background-color","Green")
			$(":submit").click(function () {
                alert("Submit button is clicked !");
            });
			 $(":password").val("xyz");
			 $(":reset").click(function () {
                alert("Reset button is clicked !");
            });
        });
    </script>
</head>

<body>
    <h1>CSS Selector: :input </h1>
    <div class="myClstest">
        Enter text :
        <input type="text" />
		<div>
